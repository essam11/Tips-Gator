<h1>All Categories</h1>
<div align="center">
  <%= will_paginate @categories, :page_link=>false  %>
<%= @categories.each do |category|%>
  <ul class="listing">
    <div class="row">
      <div class="well col-md-4 col-md-offset-4">
        <li class="category-title">
    <%=link_to "#{category.name}" , category_path(category)%>
        </li>
      </div>
    </div>
  </ul>

<%end%>
  <div align="center">
    <%= will_paginate%>
  </div>

</div>